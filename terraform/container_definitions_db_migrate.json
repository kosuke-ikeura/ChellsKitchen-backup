[
    {
        "name": "app",
        "image": "807363467264.dkr.ecr.ap-northeast-1.amazonaws.com/ck-ecr-rails",
        "cpu": 200,
        "memory": null,
        "memoryReservation": 512,
        "essential": true,
        "command": [
            "bundle",
            "exec",
            "rails",
            "db:migrate"
        ],
        "logConfiguration": {
            "logDriver": "awslogs",
            "options": {
                "awslogs-group": "/ecs/chellskitchen-logs",
                "awslogs-region": "ap-northeast-1",
                "awslogs-stream-prefix": "db-create"
            }
        },
        "environment": [
            {
                "name": "RAILS_ENV",
                "value": "development"
            },
            {
                "name": "DB_HOST",
                "value": "${db_host}"
            },
            {
                "name": "DB_USERNAME",
                "value": "${db_username}"
            },
            {
                "name": "DB_PASSWORD",
                "value": "${db_password}"
            },
            {
                "name": "DB_DATABASE",
                "value": "${db_database}"
            },
            {
                "name": "RAILS_MASTER_KEY",
                "value": "${rails_master_key}"
            }
        ]
    }
]